{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
    "react-app",
    "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "class-methods-use-this": "off"
  }
}
